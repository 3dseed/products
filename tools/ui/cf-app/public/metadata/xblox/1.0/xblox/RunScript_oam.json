{
  "id": "http://dojotoolkit.org/xblox/RunScript",
  "name": "xblox.RunScript",
  "spec": "1.0",
  "version": "1.7",
  "require": [{
    "type": "javascript-module",
    "format": "amd",
    "src": "xblox/RunScript",
    "$library": "xblox"
  }],
  "library": {

  },
  "property": {
    "stop": {
      "datatype": "boolean"
    },
    "bidirectional": {
      "datatype": "boolean",
      "title": "2 Way Binding"
    },
    "mode": {
      "datatype": "number",
      "title": "Operation Mode"
    },
    "targetevent": {
      "datatype": "string",
      "title": "Target Event"
    },
    "targetproperty": {
      "datatype": "string",
      "title": "Target Property"
    },
    "targetfilter": {
      "datatype": "number",
      "title": "Target Property Filter"
    },
    "sourceevent": {
      "datatype": "string",
      "title": "Source Event"
    },
    "sourceeventvaluepath": {
      "datatype": "string",
      "title": "Source Event Value Path",
      "readonly": false
    },
    "sourceeventnamepath": {
      "datatype": "string",
      "title": "Source Event Name Path",
      "readonly": false
    },
    "targetvariable": {
      "datatype": "string",
      "title": "Target Variable"
    },
    "block": {
      "datatype": "string",
      "readonly": true,
      "title": "Block Path"
    },
    "script": {
      "datatype": "string",
      "readonly": true,
      "title": "Script"

    },
    "enabled": {
      "datatype": "boolean",
      "defaultValue": true
    }
  },
  "ignorePosition": true,
  "delite": true,
  "content": "<d-xscript></d-xscript>",
  "title": {
    "type": "text/html",
    "value": "<p>Non-templated BUTTON widget with a thin API wrapper for click \nevents and for setting the label.\n\n</p>\n<p>Buttons can display a label, an icon, or both.\nA label should always be specified (through innerHTML) or the label\nattribute.  It can be hidden via showLabel=false.</p>"
  },
  "examples": {
    "type": "text/html",
    "value": "<pre><code>&lt;button data-dojo-type=&quot;xide/mobile/Button&quot; onClick=&quot;...&quot;&gt;Hello world&lt;/button&gt;</code></pre>"
  }
}
